{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "tsc:w": "npx tsc -w",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon ./dist/server.js --port=1488",
    "start:all": "concurrently \"npm run start:dev --port=1488\" \"npm run tsc:w\"",
    "lint:fix": "eslint --fix **/*.js"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "joi": "^17.12.1",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "concurrently": "^8.2.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "nodemon": "2.0.15"
  }
}
